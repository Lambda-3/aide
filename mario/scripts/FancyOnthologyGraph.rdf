classes:person
    rdfs:label "Person";
    rdf:type rdf:Class.

classes:posture
    rdfs:label "Posture";
    rdf:type rdf:Class.    

postures:standing
    rdfs:label "Posture";
    rdf:type classes:posture.
    
postures:fell
    rdfs:label "Posture";
    rdf:type classes:posture.
    
postures:walking
    rdfs:label "Posture";
    rdf:type classes:posture.
    
classes:skeleton
    rdfs:label "Skeleton";
    rdf:type rdf:Class.

properties:mobileNumber
    rdf:type rdf:Property;
    rdfs:label "mobile Number";
    rdfs:domain classes:person;
    rdfs:range xsd:string.

properties:skeleton
    rdf:type rdf:Property;
    rdfs:label "skeleton";
    rdfs:domain classes:person;
    rdfs:range classes:skeleton.

properties:posture
    rdf:type rdf:Property;
    rdfs:label "mobile Number";
    rdfs:domain classes:skeleton;
    rdfs:range classes:posture.
    
properties:iceContact
    rdf:type rdf:Property;
    rdfs:label "ICE contact";
    rdfs:domain classes:person;
    rdfs:range classes:person.

